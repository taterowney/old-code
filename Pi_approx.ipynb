    "import random\n",
    "in_square = in_circle = pi = 0\n",
    "for i in range(10000000):\n",
    "    x = random.random()\n",
    "    y = random.random()\n",
    "    dist = (x*x+y*y)**0.5\n",
    "    in_square+=1\n",
    "    if dist <= 1.0:\n",
    "        in_circle+=1\n",
    "pi=4*in_circle/in_square\n",
    "print(pi)"
   ]
